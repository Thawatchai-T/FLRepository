<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" default-autowire="constructor">
  <object id="sessionFactory"
        type="NHibernate.ISessionFactory, NHibernate"
        factory-object="nhHelpersFactory"
        factory-method="CreateSessionFactory" singleton="true"/>

  <object id="usersAuthorizeRepository"
              type="KTBLeasing.FrontLeasing.Mapping.Orcl.Reposotory.UsersAuthorizeRepository, KTBLeasing.FrontLeasing.Mapping.Orcl">
    <property name="SessionFactory"  ref="sessionFactory"/>
  </object>

  <object id="userInRoleRepository"
              type="KTBLeasing.FrontLeasing.Mapping.Orcl.Reposotory.UserInRoleRepository, KTBLeasing.FrontLeasing.Mapping.Orcl">
    <property name="SessionFactory"  ref="sessionFactory"/>
  </object>

  <object id="roleRepository"
              type="KTBLeasing.FrontLeasing.Mapping.Orcl.Reposotory.RoleRepository, KTBLeasing.FrontLeasing.Mapping.Orcl">
    <property name="SessionFactory"  ref="sessionFactory"/>
  </object>
  
  <object id="userInTabRepository"
              type="KTBLeasing.FrontLeasing.Mapping.Orcl.Reposotory.UserInTabRepository, KTBLeasing.FrontLeasing.Mapping.Orcl">
    <property name="SessionFactory"  ref="sessionFactory"/>
  </object>
  
  <object id="tabRepository"
            type="KTBLeasing.FrontLeasing.Mapping.Orcl.Reposotory.TabRepository, KTBLeasing.FrontLeasing.Mapping.Orcl">
    <property name="SessionFactory"  ref="sessionFactory"/>
  </object>
  
  <object id="commonDataRepository"
            type="KTBLeasing.FrontLeasing.Mapping.Orcl.Reposotory.CommonDataRepository, KTBLeasing.FrontLeasing.Mapping.Orcl">
    <property name="SessionFactory"  ref="sessionFactory"/>
  </object>

  <object id="ComboboxRepository"
            type="KTBLeasing.FrontLeasing.Mapping.Orcl.Reposotory.ComboboxRepository, KTBLeasing.FrontLeasing.Mapping.Orcl">
    <property name="SessionFactory"  ref="sessionFactory"/>
  </object>
  
</objects>
