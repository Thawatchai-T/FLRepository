    <client>
      <endpoint address="http://221.23.1.11/loginad/ws_loginad.exe/soap/IWS_LoginAD" binding="basicHttpBinding" bindingConfiguration="IWS_LoginADbinding" contract="WsLoginAD.IWS_LoginAD" name="IWS_LoginADPort"/>
      <endpoint address="http://phutip_pr-it-pc.ktbleasing.com/PolymathCoreDllService_V1.0/PolymathCoreDllService.svc?wsdl" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPolymathCoreDllService" contract="PolymathCoreDllService.IPolymathCoreDllService" name="BasicHttpBinding_IPolymathCoreDllService"/>
    </client>
  </system.serviceModel>
